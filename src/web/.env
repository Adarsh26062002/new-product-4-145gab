# These values can be overridden by environment-specific .env files

# React environment
NODE_ENV=development

# Application information
REACT_APP_NAME=React Todo List
REACT_APP_VERSION=1.0.0

# Storage configuration
REACT_APP_STORAGE_PREFIX=react-todo-list
REACT_APP_MAX_TASKS=1000

# Feature flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false

# Build configuration
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=false
DISABLE_ESLINT_PLUGIN=false
TSC_COMPILE_ON_ERROR=false